                                    "$ref": "ThemeIcons"
                                },
                                "minItems": 1,
                                "optional": true,
                                "type": "array"
                            },
                            "type": {
                                "default": "button",
                                "description": "Specifies the type of the button. Default type is <var>button</var>.",
                                "enum": [
                                    "button",
                                    "menu"
                                ],
                                "optional": true,
                                "type": "string"
                            }
                        },
                        "type": "object"
                    }
