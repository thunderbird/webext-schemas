[
    {
        "namespace": "manifest",
        "types": [
            {
                "$extend": "OptionalPermission",
                "choices": [
                    {
                        "enum": [
                            "messagesDelete",
                            "messagesImport",
                            "messagesMove",
                            "messagesRead",
                            "messagesTags",
                            "sensitiveDataUpload"
                        ],
                        "type": "string"
                    }
                ]
            }
        ]
    },
    {
        "events": [
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#onupdated-message-changedproperties",
                "description": "Fired when one or more properties of a message have been updated.",
                "name": "onUpdated",
                "parameters": [
                    {
                        "$ref": "messages.MessageHeader",
                        "name": "message"
                    },
                    {
                        "$ref": "messages.MessageProperties",
                        "name": "changedProperties"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#onmoved-originalmessages-movedmessages",
                "description": "Fired when messages have been moved.",
                "name": "onMoved",
                "parameters": [
                    {
                        "$ref": "messages.MessageList",
                        "name": "originalMessages"
                    },
                    {
                        "$ref": "messages.MessageList",
                        "name": "movedMessages"
                    }
                ],
                "permissions": [
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#oncopied-originalmessages-copiedmessages",
                "description": "Fired when messages have been copied.",
                "name": "onCopied",
                "parameters": [
                    {
                        "$ref": "messages.MessageList",
                        "name": "originalMessages"
                    },
                    {
                        "$ref": "messages.MessageList",
                        "name": "copiedMessages"
                    }
                ],
                "permissions": [
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#ondeleted-messages",
                "description": "Fired when messages have been permanently deleted.",
                "name": "onDeleted",
                "parameters": [
                    {
                        "$ref": "messages.MessageList",
                        "name": "messages"
                    }
                ],
                "permissions": [
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#onnewmailreceived-folder-messages",
                "description": "Fired when a new message is received, and has been through junk classification and message filters.",
                "name": "onNewMailReceived",
                "parameters": [
                    {
                        "$ref": "folders.MailFolder",
                        "name": "folder"
                    },
                    {
                        "$ref": "messages.MessageList",
                        "name": "messages"
                    }
                ],
                "permissions": [
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            }
        ],
        "functions": [
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#list-folder",
                "async": "callback",
                "description": "Gets all messages in a folder.",
                "name": "list",
                "parameters": [
                    {
                        "$ref": "folders.MailFolder",
                        "name": "folder"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MessageList"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "permissions": [
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#continuelist-messagelistid",
                "async": "callback",
                "description": "Returns the next chunk of messages in a list. See $(doc:how-to/messageLists) for more information.",
                "name": "continueList",
                "parameters": [
                    {
                        "name": "messageListId",
                        "type": "string"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MessageList"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#get-messageid",
                "async": "callback",
                "description": "Returns a specified message.",
                "name": "get",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MessageHeader"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#getfull-messageid",
                "async": "callback",
                "description": "Returns a specified message, including all headers and MIME parts. Throws if the message could not be read, for example due to network issues.",
                "name": "getFull",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MessagePart"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#getraw-messageid-options",
                "async": "callback",
                "description": "Returns the unmodified source of a message. Throws if the message could not be read, for example due to network issues.",
                "name": "getRaw",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "options",
                        "optional": true,
                        "properties": {
                            "data_format": {
                                "description": "The message can either be returned as a DOM File or as a <a href='|link-binary-string|'>binary string</a>. The historic default is to return a binary string (kept for backward compatibility). However, it is now recommended to use the <val>File</val> format, because the DOM File object can be used as-is with the downloads API and has useful methods to access the content, like <a href='|link-DOMFile-text|'>File.text()</a> and <a href='|link-DOMFile-arrayBuffer|'>File.arrayBuffer()</a>. Working with binary strings is error prone and needs special handling: <literalinclude>includes/messages/decodeBinaryString.js<lang>JavaScript</lang></literalinclude> (see MDN for <a href='|link-input-encoding|'>supported input encodings</a>).",
                                "enum": [
                                    "BinaryString",
                                    "File"
                                ],
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "choices": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "additionalProperties": true,
                                        "isInstanceOf": "File",
                                        "type": "object"
                                    }
                                ]
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#listattachments-messageid",
                "async": "callback",
                "description": "Lists the attachments of a message.",
                "name": "listAttachments",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "items": {
                                    "$ref": "MessageAttachment"
                                },
                                "type": "array"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#getattachmentfile-messageid-partname",
                "async": "callback",
                "description": "Gets the content of a $(ref:messages.MessageAttachment) as a |File| object.",
                "name": "getAttachmentFile",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "partName",
                        "pattern": "^\\d+(\\.\\d+)*$",
                        "type": "string"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "additionalProperties": true,
                                "isInstanceOf": "File",
                                "type": "object"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#openattachment-messageid-partname-tabid",
                "async": true,
                "description": "Opens the specified attachment",
                "name": "openAttachment",
                "parameters": [
                    {
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "name": "partName",
                        "pattern": "^\\d+(\\.\\d+)*$",
                        "type": "string"
                    },
                    {
                        "description": "The ID of the tab associated with the message opening.",
                        "name": "tabId",
                        "type": "integer"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#query-queryinfo",
                "async": "callback",
                "description": "Gets all messages that have the specified properties, or all messages if no properties are specified.",
                "name": "query",
                "parameters": [
                    {
                        "default": {},
                        "name": "queryInfo",
                        "optional": true,
                        "properties": {
                            "attachment": {
                                "description": "If specified, returns only messages with or without attachments.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "author": {
                                "description": "Returns only messages with this value matching the author. The search value is a single email address, a name or a combination (e.g.: <value>Name <user@domain.org></value>). The address part of the search value (if provided) must match the author's address completely. The name part of the search value (if provided) must match the author's name partially. All matches are done case-insensitive.",
                                "optional": true,
                                "type": "string"
                            },
                            "body": {
                                "description": "Returns only messages with this value in the body of the mail.",
                                "optional": true,
                                "type": "string"
                            },
                            "flagged": {
                                "description": "Returns only flagged (or unflagged if false) messages.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "folder": {
                                "$ref": "folders.MailFolder",
                                "description": "Returns only messages from the specified folder. The <permission>accountsRead</permission> permission is required.",
                                "optional": true
                            },
                            "fromDate": {
                                "$ref": "extensionTypes.Date",
                                "description": "Returns only messages with a date after this value.",
                                "optional": true
                            },
                            "fromMe": {
                                "description": "Returns only messages with the author's address matching any configured identity.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "fullText": {
                                "description": "Returns only messages with this value somewhere in the mail (subject, body or author).",
                                "optional": true,
                                "type": "string"
                            },
                            "headerMessageId": {
                                "description": "Returns only messages with a Message-ID header matching this value.",
                                "optional": true,
                                "type": "string"
                            },
                            "includeSubFolders": {
                                "description": "Search the folder specified by <val>queryInfo.folder</val> recursively.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "recipients": {
                                "description": "Returns only messages whose recipients match all specified addresses. The search value is a semicolon separated list of email addresses, names or combinations (e.g.: <value>Name <user@domain.org></value>). For a match, all specified addresses must equal a recipient's address completely and all specified names must match a recipient's name partially. All matches are done case-insensitive.",
                                "optional": true,
                                "type": "string"
                            },
                            "subject": {
                                "description": "Returns only messages with this value matching the subject.",
                                "optional": true,
                                "type": "string"
                            },
                            "tags": {
                                "$ref": "TagsDetail",
                                "description": "Returns only messages with the specified tags. For a list of available tags, call the $(ref:messages.listTags) method.",
                                "optional": true
                            },
                            "toDate": {
                                "$ref": "extensionTypes.Date",
                                "description": "Returns only messages with a date before this value.",
                                "optional": true
                            },
                            "toMe": {
                                "description": "Returns only messages with at least one recipient address matching any configured identity.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "unread": {
                                "description": "Returns only unread (or read if false) messages.",
                                "optional": true,
                                "type": "boolean"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MessageList"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#update-messageid-newproperties",
                "async": true,
                "description": "Marks or unmarks a message as junk, read, flagged, or tagged. Updating external messages will throw an <em>ExtensionError</em>.",
                "name": "update",
                "parameters": [
                    {
                        "minimum": 1,
                        "name": "messageId",
                        "type": "integer"
                    },
                    {
                        "$ref": "MessageProperties",
                        "name": "newProperties"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#move-messageids-destination",
                "async": true,
                "description": "Moves messages to a specified folder. If the messages cannot be removed from the source folder, they will be copied instead of moved. Moving external messages will throw an <em>ExtensionError</em>.",
                "name": "move",
                "parameters": [
                    {
                        "description": "The IDs of the messages to move.",
                        "items": {
                            "minimum": 1,
                            "type": "integer"
                        },
                        "name": "messageIds",
                        "type": "array"
                    },
                    {
                        "$ref": "folders.MailFolder",
                        "description": "The folder to move the messages to.",
                        "name": "destination"
                    }
                ],
                "permissions": [
                    "accountsRead",
                    "messagesMove"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#copy-messageids-destination",
                "async": true,
                "description": "Copies messages to a specified folder.",
                "name": "copy",
                "parameters": [
                    {
                        "description": "The IDs of the messages to copy.",
                        "items": {
                            "minimum": 1,
                            "type": "integer"
                        },
                        "name": "messageIds",
                        "type": "array"
                    },
                    {
                        "$ref": "folders.MailFolder",
                        "description": "The folder to copy the messages to.",
                        "name": "destination"
                    }
                ],
                "permissions": [
                    "accountsRead",
                    "messagesMove"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#delete-messageids-skiptrash",
                "async": true,
                "description": "Deletes messages permanently, or moves them to the trash folder (honoring the account's deletion behavior settings). Deleting external messages will throw an <em>ExtensionError</em>. The <val>skipTrash</val> parameter allows immediate permanent deletion, bypassing the trash folder.\n**Note**: Consider using $(ref:messages.move) to manually move messages to the account's trash folder, instead of requesting the overly powerful permission to actually delete messages. The account's trash folder can be extracted as follows: <literalinclude>includes/messages/getTrash.js<lang>JavaScript</lang></literalinclude>",
                "name": "delete",
                "parameters": [
                    {
                        "description": "The IDs of the messages to delete.",
                        "items": {
                            "minimum": 1,
                            "type": "integer"
                        },
                        "name": "messageIds",
                        "type": "array"
                    },
                    {
                        "description": "If true, the message will be deleted permanently, regardless of the account's deletion behavior settings.",
                        "name": "skipTrash",
                        "optional": true,
                        "type": "boolean"
                    }
                ],
                "permissions": [
                    "messagesDelete"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#import-file-destination-properties",
                "async": "callback",
                "description": "Imports a message into a local Thunderbird folder. To import a message into an IMAP folder, add it to a local folder first and then move it to the IMAP folder.",
                "name": "import",
                "parameters": [
                    {
                        "additionalProperties": true,
                        "isInstanceOf": "File",
                        "name": "file",
                        "type": "object"
                    },
                    {
                        "$ref": "folders.MailFolder",
                        "description": "The folder to import the messages into.",
                        "name": "destination"
                    },
                    {
                        "$ref": "messages.MessageProperties",
                        "name": "properties",
                        "optional": true
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "messages.MessageHeader"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "permissions": [
                    "accountsRead",
                    "messagesImport"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#archive-messageids",
                "async": true,
                "description": "Archives messages using the current settings. Archiving external messages will throw an <em>ExtensionError</em>.",
                "name": "archive",
                "parameters": [
                    {
                        "description": "The IDs of the messages to archive.",
                        "items": {
                            "minimum": 1,
                            "type": "integer"
                        },
                        "name": "messageIds",
                        "type": "array"
                    }
                ],
                "permissions": [
                    "messagesMove"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#listtags",
                "async": "callback",
                "description": "Returns a list of tags that can be set on messages, and their human-friendly name, colour, and sort order.",
                "name": "listTags",
                "parameters": [
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "items": {
                                    "$ref": "MessageTag"
                                },
                                "type": "array"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#createtag-key-tag-color",
                "async": true,
                "description": "Creates a new message tag. Tagging a message will store the tag's key in the user's message. Throws if the specified tag key is used already.",
                "name": "createTag",
                "parameters": [
                    {
                        "description": "Unique tag identifier (will be converted to lower case). Must not include <value>()<>{/%*\"</value> or spaces.",
                        "name": "key",
                        "pattern": "^[^ ()/{%*<>\"]+$",
                        "type": "string"
                    },
                    {
                        "description": "Human-readable tag name.",
                        "name": "tag",
                        "type": "string"
                    },
                    {
                        "description": "Tag color in hex format (i.e.: #000080 for navy blue)",
                        "name": "color",
                        "pattern": "^#[0-9a-f]{6}",
                        "type": "string"
                    }
                ],
                "permissions": [
                    "messagesTags"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#updatetag-key-updateproperties",
                "async": true,
                "description": "Updates a message tag.",
                "name": "updateTag",
                "parameters": [
                    {
                        "description": "Unique tag identifier (will be converted to lower case). Must not include <value>()<>{/%*\"</value> or spaces.",
                        "name": "key",
                        "pattern": "^[^ ()/{%*<>\"]+$",
                        "type": "string"
                    },
                    {
                        "name": "updateProperties",
                        "properties": {
                            "color": {
                                "description": "Tag color in hex format (i.e.: #000080 for navy blue).",
                                "optional": "true",
                                "pattern": "^#[0-9a-f]{6}",
                                "type": "string"
                            },
                            "tag": {
                                "description": "Human-readable tag name.",
                                "optional": "true",
                                "type": "string"
                            }
                        },
                        "type": "object"
                    }
                ],
                "permissions": [
                    "messagesTags"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115/messages.html#deletetag-key",
                "async": true,
                "description": "Deletes a message tag, removing it from the list of known tags. Its key will not be removed from tagged messages, but they will appear untagged. Recreating a deleted tag, will make all former tagged messages appear tagged again.",
                "name": "deleteTag",
                "parameters": [
                    {
                        "description": "Unique tag identifier (will be converted to lower case). Must not include <value>()<>{/%*\"</value> or spaces.",
                        "name": "key",
                        "pattern": "^[^ ()/{%*<>\"]+$",
                        "type": "string"
                    }
                ],
                "permissions": [
                    "messagesTags"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            }
        ],
        "namespace": "messages",
        "permissions": [
            "messagesRead"
        ],
        "types": [
            {
                "description": "Basic information about a message.",
                "id": "MessageHeader",
                "properties": {
                    "author": {
                        "type": "string"
                    },
                    "bccList": {
                        "description": "The Bcc recipients. Not populated for news/nntp messages.",
                        "items": {
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "ccList": {
                        "description": "The Cc recipients. Not populated for news/nntp messages.",
                        "items": {
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "date": {
                        "$ref": "extensionTypes.Date"
                    },
                    "external": {
                        "description": "Whether this message is a real message or an external message (opened from a file or from an attachment).",
                        "type": "boolean"
                    },
                    "flagged": {
                        "description": "Whether this message is flagged (a.k.a. starred).",
                        "type": "boolean"
                    },
                    "folder": {
                        "$ref": "folders.MailFolder",
                        "description": "The <permission>accountsRead</permission> permission is required for this property to be included. Not available for external or attached messages.",
                        "optional": true
                    },
                    "headerMessageId": {
                        "description": "The message-id header of the message.",
                        "type": "string"
                    },
                    "headersOnly": {
