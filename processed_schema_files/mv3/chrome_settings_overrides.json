[
    {
        "namespace": "manifest",
        "types": [
            {
                "$extend": "WebExtensionManifest",
                "properties": {
                    "chrome_settings_overrides": {
                        "additionalProperties": {
                            "$ref": "UnrecognizedProperty"
                        },
                        "optional": true,
                        "properties": {
                            "search_provider": {
                                "additionalProperties": {
                                    "$ref": "UnrecognizedProperty"
                                },
                                "optional": true,
                                "properties": {
                                    "alternate_urls": {
                                        "deprecated": "Unsupported on Thunderbird at this time.",
                                        "items": {
                                            "format": "url",
                                            "preprocess": "localize",
                                            "type": "string"
                                        },
                                        "optional": true,
                                        "type": "array"
                                    },
                                    "encoding": {
                                        "description": "Encoding of the search term.",
                                        "optional": true,
                                        "type": "string"
                                    },
                                    "favicon_url": {
                                        "format": "url",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "image_url": {
                                        "deprecated": "Unsupported on Thunderbird at this time.",
                                        "format": "url",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "image_url_post_params": {
                                        "deprecated": "Unsupported on Thunderbird at this time.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "instant_url": {
                                        "deprecated": "Unsupported on Thunderbird at this time.",
                                        "format": "url",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "instant_url_post_params": {
                                        "deprecated": "Unsupported on Thunderbird at this time.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "is_default": {
                                        "description": "Sets the default engine to a built-in engine only.",
                                        "optional": true,
                                        "type": "boolean"
                                    },
                                    "keyword": {
                                        "choices": [
                                            {
                                                "preprocess": "localize",
                                                "type": "string"
                                            },
                                            {
                                                "items": {
                                                    "preprocess": "localize",
                                                    "type": "string"
                                                },
                                                "minItems": 1,
                                                "type": "array"
                                            }
                                        ],
                                        "optional": true
                                    },
                                    "name": {
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "params": {
                                        "description": "A list of optional search url parameters. This allows the addition of search url parameters based on how the search is performed in Thunderbird.",
                                        "items": {
                                            "properties": {
                                                "condition": {
                                                    "description": "The type of param can be either \"purpose\" or \"pref\".",
                                                    "enum": [
                                                        "purpose",
                                                        "pref"
                                                    ],
                                                    "optional": true,
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "description": "A url parameter name",
                                                    "type": "string"
                                                },
                                                "pref": {
                                                    "description": "The preference to retrieve the value from.",
                                                    "optional": true,
                                                    "type": "string"
                                                },
                                                "purpose": {
                                                    "description": "The context that initiates a search, required if condition is \"purpose\".",
                                                    "enum": [
                                                        "contextmenu",
                                                        "searchbar",
                                                        "homepage",
                                                        "keyword",
                                                        "newtab"
                                                    ],
                                                    "optional": true,
                                                    "type": "string"
                                                },
                                                "value": {
                                                    "description": "A url parameter value.",
                                                    "optional": true,
                                                    "preprocess": "localize",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "optional": true,
                                        "type": "array"
                                    },
                                    "prepopulated_id": {
                                        "deprecated": "Unsupported on Thunderbird.",
                                        "optional": true,
                                        "type": "integer"
                                    },
                                    "search_form": {
                                        "format": "url",
                                        "optional": true,
                                        "pattern": "^https://.*$",
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "search_url": {
                                        "format": "url",
                                        "pattern": "^https://.*$",
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "search_url_get_params": {
                                        "description": "GET parameters to the search_url as a query string.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "search_url_post_params": {
                                        "description": "POST parameters to the search_url as a query string.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "suggest_url": {
                                        "optional": true,
                                        "pattern": "^https://.*$|^$",
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "suggest_url_get_params": {
                                        "description": "GET parameters to the suggest_url as a query string.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    },
                                    "suggest_url_post_params": {
                                        "description": "POST parameters to the suggest_url as a query string.",
                                        "optional": true,
                                        "preprocess": "localize",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "type": "object"
                    }
                }
            }
        ]
    }
]