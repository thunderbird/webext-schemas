                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#onselectedmessageschanged-tab-selectedmessages",
                "description": "Fired when the selected messages change in any mail tab.",
                "name": "onSelectedMessagesChanged",
                "parameters": [
                    {
                        "$ref": "tabs.Tab",
                        "name": "tab",
                        "support": {
                            "version_added": true
                        }
                    },
