        "functions": [
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#query-queryinfo",
                "async": "callback",
                "description": "Gets all mail tabs that have the specified properties, or all mail tabs if no properties are specified.",
                "name": "query",
                "parameters": [
                    {
                        "default": {},
                        "name": "queryInfo",
                        "optional": true,
                        "properties": {
                            "active": {
                                "description": "Whether the tabs are active in their windows.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "currentWindow": {
                                "description": "Whether the tabs are in the current window.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "lastFocusedWindow": {
                                "description": "Whether the tabs are in the last focused window.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "windowId": {
                                "description": "The ID of the parent window, or $(ref:windows.WINDOW_ID_CURRENT) for the current window.",
                                "minimum": -2,
                                "optional": true,
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "items": {
                                    "$ref": "MailTab"
                                },
                                "type": "array"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#get-tabid",
                "async": "callback",
                "description": "Get the properties of a mail tab.",
                "name": "get",
                "parameters": [
                    {
                        "description": "ID of the requested mail tab. Throws if the requested tab is not a mail tab.",
                        "name": "tabId",
                        "type": "integer"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MailTab"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#getcurrent",
                "async": "callback",
                "description": "Get the properties of the active mail tab, if the active tab is a mail tab. Returns undefined otherwise.",
                "name": "getCurrent",
                "parameters": [
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "MailTab",
                                "optional": true
                            }
                        ],
                        "type": "function"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#update-tabid-updateproperties",
                "async": true,
                "description": "Modifies the properties of a mail tab. Properties that are not specified in <val>updateProperties</val> are not modified.",
                "name": "update",
                "parameters": [
                    {
                        "description": "Defaults to the active tab of the current window.",
                        "minimum": 1,
                        "name": "tabId",
                        "optional": true,
                        "type": "integer"
                    },
                    {
                        "name": "updateProperties",
                        "properties": {
                            "displayedFolder": {
                                "$ref": "folders.MailFolder",
                                "description": "Sets the folder displayed in the tab. The extension must have the <permission>accountsRead</permission> permission to do this. The previous message selection in the given folder will be restored.",
                                "optional": true
                            },
                            "folderPaneVisible": {
                                "description": "Shows or hides the folder pane.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "layout": {
                                "description": "Sets the arrangement of the folder pane, message list pane, and message display pane. Note that setting this applies it to all mail tabs.",
                                "enum": [
                                    "standard",
                                    "wide",
                                    "vertical"
                                ],
                                "optional": true,
                                "type": "string"
                            },
                            "messagePaneVisible": {
                                "description": "Shows or hides the message display pane.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "sortOrder": {
                                "description": "Sorts the list of messages. <val>sortType</val> must also be given.",
                                "enum": [
                                    "none",
                                    "ascending",
                                    "descending"
                                ],
                                "optional": true,
                                "type": "string"
                            },
                            "sortType": {
                                "description": "Sorts the list of messages. <val>sortOrder</val> must also be given.",
                                "enum": [
                                    "none",
                                    "date",
                                    "subject",
                                    "author",
                                    "id",
                                    "thread",
                                    "priority",
                                    "status",
                                    "size",
                                    "flagged",
                                    "unread",
                                    "recipient",
                                    "location",
                                    "tags",
                                    "junkStatus",
                                    "attachments",
                                    "account",
                                    "custom",
                                    "received",
                                    "correspondent"
                                ],
                                "optional": true,
                                "type": "string"
                            },
                            "viewType": {
                                "enum": [
                                    "ungrouped",
                                    "groupedByThread",
                                    "groupedBySortType"
                                ],
                                "optional": true,
                                "type": "string"
                            }
                        },
                        "type": "object"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#getselectedmessages-tabid",
                "async": "callback",
                "description": "Lists the selected messages in the current folder.",
                "name": "getSelectedMessages",
                "parameters": [
                    {
                        "description": "Defaults to the active tab of the current window.",
                        "minimum": 1,
                        "name": "tabId",
                        "optional": true,
                        "type": "integer"
                    },
                    {
                        "name": "callback",
                        "optional": true,
                        "parameters": [
                            {
                                "$ref": "messages.MessageList"
                            }
                        ],
                        "type": "function"
                    }
                ],
                "permissions": [
                    "messagesRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#setselectedmessages-tabid-messageids",
                "async": true,
                "description": "Selects none, one or multiple messages.",
                "name": "setSelectedMessages",
                "parameters": [
                    {
                        "description": "Defaults to the active tab of the current window.",
                        "minimum": 1,
                        "name": "tabId",
                        "optional": true,
                        "type": "integer"
                    },
                    {
                        "description": "The IDs of the messages, which should be selected. The mailTab will switch to the folder of the selected messages. Throws if they belong to different folders. Array can be empty to deselect any currently selected message.",
                        "items": {
                            "type": "integer"
                        },
                        "name": "messageIds",
                        "type": "array"
                    }
                ],
                "permissions": [
                    "messagesRead",
                    "accountsRead"
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            },
            {
                "api_documentation_url": "https://webextension-api.thunderbird.net/en/115-mv3/mailTabs.html#setquickfilter-tabid-properties",
                "async": true,
                "description": "Sets the Quick Filter user interface based on the options specified.",
                "name": "setQuickFilter",
                "parameters": [
                    {
                        "description": "Defaults to the active tab of the current window.",
                        "minimum": 1,
                        "name": "tabId",
                        "optional": true,
                        "type": "integer"
                    },
                    {
                        "name": "properties",
                        "properties": {
                            "attachment": {
                                "description": "Shows only messages with attachments.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "contact": {
                                "description": "Shows only messages from people in the address book.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "flagged": {
                                "description": "Shows only flagged messages.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "show": {
                                "description": "Shows or hides the Quick Filter bar.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "tags": {
                                "choices": [
                                    {
                                        "type": "boolean"
                                    },
                                    {
                                        "$ref": "messages.TagsDetail"
                                    }
                                ],
                                "description": "Shows only messages with tags on them.",
                                "optional": true
                            },
                            "text": {
                                "$ref": "QuickFilterTextDetail",
                                "description": "Shows only messages matching the supplied text.",
                                "optional": true
                            },
                            "unread": {
                                "description": "Shows only unread messages.",
                                "optional": true,
                                "type": "boolean"
                            }
                        },
                        "type": "object"
                    }
                ],
                "support": {
                    "version_added": true
                },
                "type": "function"
            }
        ],
        "namespace": "mailTabs",
        "types": [
            {
                "id": "MailTab",
                "properties": {
                    "active": {
                        "type": "boolean"
                    },
                    "displayedFolder": {
                        "$ref": "folders.MailFolder",
                        "description": "The <permission>accountsRead</permission> permission is required for this property to be included.",
                        "optional": true
                    },
                    "folderPaneVisible": {
                        "optional": true,
                        "type": "boolean"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "layout": {
                        "enum": [
                            "standard",
                            "wide",
                            "vertical"
                        ],
                        "type": "string"
                    },
                    "messagePaneVisible": {
                        "optional": true,
                        "type": "boolean"
                    },
                    "sortOrder": {
                        "description": "**Note:** <val>sortType</val> and <val>sortOrder</val> depend on each other, so both should be present, or neither.",
                        "enum": [
                            "none",
                            "ascending",
                            "descending"
                        ],
                        "optional": true,
                        "type": "string"
                    },
                    "sortType": {
                        "description": "**Note:** <val>sortType</val> and <val>sortOrder</val> depend on each other, so both should be present, or neither.",
                        "enum": [
                            "none",
                            "date",
                            "subject",
                            "author",
                            "id",
                            "thread",
                            "priority",
                            "status",
                            "size",
                            "flagged",
                            "unread",
                            "recipient",
                            "location",
                            "tags",
                            "junkStatus",
                            "attachments",
                            "account",
                            "custom",
                            "received",
                            "correspondent"
                        ],
                        "optional": true,
                        "type": "string"
                    },
                    "viewType": {
                        "enum": [
                            "ungrouped",
                            "groupedByThread",
                            "groupedBySortType"
                        ],
                        "optional": true,
                        "type": "string"
                    },
                    "windowId": {
                        "type": "integer"
                    }
                },
                "type": "object"
            },
            {
                "id": "QuickFilterTextDetail",
                "properties": {
                    "author": {
                        "description": "Shows messages where <val>text</val> matches the author.",
                        "optional": true,
                        "type": "boolean"
                    },
                    "body": {
                        "description": "Shows messages where <val>text</val> matches the message body.",
                        "optional": true,
                        "type": "boolean"
                    },
                    "recipients": {
                        "description": "Shows messages where <val>text</val> matches the recipients.",
                        "optional": true,
                        "type": "boolean"
                    },
                    "subject": {
                        "description": "Shows messages where <val>text</val> matches the subject.",
                        "optional": true,
                        "type": "boolean"
                    },
                    "text": {
                        "description": "String to match against the <val>recipients</val>, <val>author</val>, <val>subject</val>, or <val>body</val>.",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        ]
    }
