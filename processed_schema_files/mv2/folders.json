                                "optional": true
                            },
                            "isFavorite": {
                                "description": "Whether the folder is a favorite folder, or not.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "isRoot": {
                                "description": "Whether the folder is a root folder, or not.",
                                "optional": true,
                                "type": "boolean"
                            },
                            "isVirtual": {
