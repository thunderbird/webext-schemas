                "parameters": [
                    {
                        "$ref": "Permissions",
                        "name": "permissions"
                    }
                ],
                "support": {
                    "version_added": "77"
                },
                "type": "function"
            }
        ],
        "functions": [
            {
                "api_documentation_url": "https://developer.mozilla.org/docs/Mozilla/Add-ons/WebExtensions/API/permissions/getAll",
